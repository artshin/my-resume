{
  "name": "artshin-resume",
  "version": "1.0.0",
  "description": "CLI tool to generate tailored PDF resumes from structured job data",
  "type": "module",
  "main": "dist/cli.js",
  "bin": {
    "resume-gen": "dist/cli.js"
  },
  "scripts": {
    "generate": "tsx src/cli.ts",
    "build": "tsc",
    "lint": "eslint src/",
    "format": "prettier --write 'src/**/*.ts'",
    "check-ollama": "tsx src/utils/check-ollama.ts",
    "validate": "tsx src/utils/validate-data.ts",
    "test-analyzer": "tsx src/utils/test-analyzer.ts",
    "test-matcher": "tsx src/utils/test-matcher.ts",
    "test-template": "tsx src/utils/test-template.ts",
    "test-pdf": "tsx src/utils/test-pdf.ts"
  },
  "keywords": [
    "resume",
    "cv",
    "pdf",
    "generator",
    "cli",
    "ollama"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "dotenv": "^16.4.5",
    "handlebars": "^4.7.8",
    "prompts": "^2.4.2",
    "puppeteer": "^23.4.0"
  },
  "devDependencies": {
    "@types/node": "^22.5.5",
    "@types/prompts": "^2.4.9",
    "@typescript-eslint/eslint-plugin": "^8.6.0",
    "@typescript-eslint/parser": "^8.6.0",
    "eslint": "^9.10.0",
    "prettier": "^3.3.3",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
